struct Point {
    x: num,
    y: num
}

struct Circle {
    center: Point,
    radius: num
}

action insertPoint(point: Point) {
    insert point;
}

action deletePoint(point: ref Point) {
    delete point;
}

mod colors {
    struct RGB {
        r: num,
        g: num,
        b: num
    }

    struct CMYK {
        c: num,
        m: num,
        y: num,
        k: num,
    }
}
